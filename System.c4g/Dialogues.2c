		[2,[0,1],"Was steht Ihr hier so rum?","Ich bin Holzfäller, aber ich habe meine Axt im Wald verloren. Jetzt kann ich nichts arbeiten.",0,0,[true,0,0,0,false,0,0,"Lumberjack"],"!DlgVar(\"A\",pTarget,pSpeaker)",["CreateDlgVar(\"A\",pTarget,pSpeaker)=1","DlgVar(\"Cancel\",pTarget,pSpeaker)=0"]],
