					  	  [15,[1,14,16,17,19],"Und jetzt?","Jetzt will ich meine Axt wieder haben. Sie war wohl doch nicht verflucht.",0,0,[true,0,0,0,false,0,0,"Lumberjack",true],["DlgVar(\"C\",pTarget,pSpeaker)","DlgVar(\"C2\",pTarget,pSpeaker)==0"],"CreateDlgVar(\"C2\",pTarget,pSpeaker)=1"],
					 	    [16,[1,14,15,17,19],"Nicht verflucht?","Die olle Hexe hat mich letztens gefragt wieso ich hier so rumstehe. Ich habe ihr alles erzählt, da hat sie mich ganz doll ausgelacht.",0,0,[true,0,0,0,false,0,0,"Lumberjack",true],["DlgVar(\"D\",pTarget,pSpeaker)==0","DlgVar(\"C2\",pTarget,pSpeaker)"],"CreateDlgVar(\"D\",pTarget,pSpeaker)=1"],
					 	  [17,[1,14,15,16,19],"Wieso kauft Ihr nicht eine neue Axt?","Sie ist mir halt so ans Herz gewachsen. Mit einer neuen Axt wäre die Arbeit nicht dieselbe.",0,0,[true,0,0,0,false,0,0,"Lumberjack"],["DlgVar(\"E\",pTarget,pSpeaker)==0","DlgVar(\"C\",pTarget,pSpeaker)"],"CreateDlgVar(\"E\",pTarget,pSpeaker)=1"],
					 	  [18,[1,14,15,16,17,19],"Ich schaue mal, ob ich Eure Axt finden kann.","Das wäre schön.",0,0,[true,0,0,0,false,0,0,"Lumberjack"],["DlgVar(\"F\",pTarget,pSpeaker)==0","DlgVar(\"C\",pTarget,pSpeaker)"],["CreateDlgVar(\"F\",pTarget,pSpeaker)=1","ActivateQuest(\"Lumberjack\",pTarget)"]],
		      [19,[5,1,14,15,16,17,18],"Wohin habt Ihr die Axt geworfen?","Das weiß ich auch nicht mehr so genau. Ich bin einfach in den Wald gegangen und habe sie so weit geworfen, wie ich konnte.",0,0,[true,0,0,0,false,0,0,"Lumberjack"],["DlgVar(\"A\",pTarget,pSpeaker)","DlgVar(\"G\",pTarget,pSpeaker)==0"],"CreateDlgVar(\"G\",pTarget,pSpeaker)=1"],
		      [20,[5,1,14,15,16,17,18],"Dann seid Ihr wohl der dümmste Holzfäller der Welt!","Pah, ich kann mir ja eine neue Axt kaufen. Lasst mich in Ruhe.",0,0,[true,0,0,0,false,0,0,"Lumberjack"],["GetQuestStage(\"Lumberjack\",pTarget)>-2","DlgVar(\"A\",pTarget,pSpeaker)","DlgVar(\"F\",pTarget,pSpeaker)==0"],["ActivateQuest(\"Lumberjack\",pTarget)","SetQuestStage(\"Lumberjack\",-2,pTarget)","StopDialogue(pTarget)"]],
